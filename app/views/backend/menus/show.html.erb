<% unless notice.nil? %>
<div class="alert alert-success" id="notice"><%= notice %></div>
<% end %>
<p>
  <strong><%= Menu.human_attribute_name('name') %>:</strong>
  <%= @menu.name %>
</p>

<p>
  <strong><%= Menu.human_attribute_name('event_type') %>:</strong>
  <%= @menu.event_type %>
</p>
<% if @menu.event_type == 'click'%>
<p>
  <strong><%= Menu.human_attribute_name('material') %>:</strong>
  <%= @menu.key_value_name %>
</p>
<% else %>
<p>
  <strong><%= Menu.human_attribute_name('url') %>:</strong>
  <%= @menu.url %>
</p>
<% end %>

<p>
  <strong><%= Menu.human_attribute_name('parent_menu') %>:</strong>
  <%= @menu.parent_menu.name unless @menu.parent_menu.blank?%>
</p>

<p>
  <strong><%= Menu.human_attribute_name('menu_type') %>:</strong>
  <%= @menu.menu_type %>
</p>

<%= link_to t('Edit'), edit_backend_shop_system_config_menu_path(@current_shop.slug,@system_config ,@menu) %> |
<%= link_to t('Back'), backend_shop_system_config_menus_path(@current_shop.slug,@system_config) %>
