%ul.tabs
  - unless @current_branch.split_supported_order_types.index(Order::ORDERTYPE_DELIVERY).nil?
    %li{class: (params[:order_type].nil? or params[:order_type] == Order::ORDERTYPE_DELIVERY ? "active percent-width-3" : "percent-width-3")}
      = link_to @custom_ui_setting.get_value(:order_delivery_btn), new_mobile_shop_branch_order_path(@current_shop.slug, @current_branch, {:order_type=>Order::ORDERTYPE_DELIVERY}), class: "first"
  - unless @current_branch.split_supported_order_types.index(Order::ORDERTYPE_EAT_IN_HALL).nil?
    %li{class: (params[:order_type] == Order::ORDERTYPE_EAT_IN_HALL ? "active percent-width-3" : "percent-width-3")}
      =link_to @custom_ui_setting.get_value(:order_eat_in_hall_btn), new_mobile_shop_branch_order_path(@current_shop.slug, @current_branch, {:order_type=>Order::ORDERTYPE_EAT_IN_HALL})
  - unless @current_branch.split_supported_order_types.index(Order::ORDERTYPE_ORDER_SEAT).nil?
    %li{class: (params[:order_type] == Order::ORDERTYPE_ORDER_SEAT ? "active percent-width-3" : "percent-width-3")}
      = link_to @custom_ui_setting.get_value(:order_order_seat), new_mobile_shop_branch_order_path(@current_shop.slug, @current_branch, {:order_type=>Order::ORDERTYPE_ORDER_SEAT}), class: "last"